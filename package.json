{
  "name": "cordova-spatialite-storage",
  "version": "2.1.0",
  "description": "Native interface to SQLite with spatialite for PhoneGap / Cordova - cordova-spatialite-storage plugin version",
  "cordova": {
    "id": "cordova-spatialite-storage",
    "platforms": [
      "android",
      "browser",
      "ios",
      "osx",
      "windows"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rfernandezf/cordova-spatialite-storage.git"
  },
  "keywords": [
    "sqlite",
    "spatialite",
    "ecosystem:cordova",
    "cordova-android",
    "cordova-browser",
    "cordova-ios",
    "cordova-osx",
    "cordova-windows"
  ],
  "author": "various",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xpbrew/cordova-sqlite-storage/issues"
  },
  "homepage": "https://github.com/xpbrew/cordova-sqlite-storage",
  "dependencies": {
    "cordova-sqlite-storage-dependencies": "4.0.0"
  },
  "scripts": {
    "clean-spec": "rm -rf spec/[mnp]* && git cl spec/config.xml spec/package.json && git status --ignored",
    "prepare-js": "coffee -p SQLitePlugin.coffee.md > www/SQLitePlugin.js",
    "prepare-spec": "node scripts/prepareSpec.js"
  },
  "devDependencies": {
    "coffeescript": "1",
    "coffescript": "^0.0.1-security",
    "cross-spawn": "^6.0.5",
    "fs-extra": "^8.1.0"
  }
}
